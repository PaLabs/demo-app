{
  "name": "PaLabs/demo-app",
  "license": "MIT",
  "type": "project",
  "description": "Example of using PaLabs platform",
  "require": {
    "php": "^7.1.3",
    "doctrine/doctrine-bundle": "^1.6.10",
    "doctrine/doctrine-fixtures-bundle": "^3.0",
    "doctrine/doctrine-migrations-bundle": "^1.3",
    "doctrine/orm": "^2.5.11",
    "sensio/framework-extra-bundle": "^5.0",
    "sensiolabs/security-checker": "^4.1",
    "symfony/symfony": "^4.0",
    "symfony/asset": "^4.0",
    "symfony/expression-language": "^4.0",
    "symfony/flex": "^1.0",
    "symfony/form": "^4.0",
    "symfony/framework-bundle": "^4.0",
    "symfony/monolog-bundle": "^3.1",
    "symfony/polyfill-apcu": "^1.5",
    "symfony/security-bundle": "^4.0",
    "symfony/translation": "^4.0",
    "symfony/twig-bundle": "^4.0",
    "symfony/validator": "^4.0",
    "symfony/yaml": "^4.0",
    "symfony/dotenv": "^4.0",
    "twig/extensions": "^1.5",
    "almasaeed2010/adminlte": "^2.3",
    "fzaninotto/faker": "^1.7",
    "phpoffice/phpexcel": "^1.8",
    "guzzlehttp/guzzle": "^6.3",
    "athari/yalinqo": "^2.4",
    "palabs/datagrid-bundle": "dev-master",
    "palabs/endpoint-bundle": "dev-master"
  },
  "require-dev": {
    "phpoffice/phpspreadsheet": "1.0.0-beta",
    "symfony/stopwatch": "^4.0",
    "symfony/web-profiler-bundle": "^4.0",
    "symfony/web-server-bundle": "^4.0"
  },
  "config": {
    "platform": {
      "php": "7.1.3"
    },
    "preferred-install": {
      "*": "dist"
    },
    "sort-packages": true
  },
  "autoload": {
    "psr-4": {
      "App\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "App\\Tests\\": "tests/"
    }
  },
  "scripts": {
    "auto-scripts": {
      "security-checker security:check": "script",
      "cache": "bin/console cache:clear --no-warmup --ansi && bin/console cache:warmup --ansi"
    }
  },
  "repositories": [
    {
      "type": "path",
      "url": "../DatagridBundle",
      "options": {
        "symlink": true
      }
    },
    {
      "type": "path",
      "url": "../EndpointBundle",
      "options": {
        "symlink": true
      }
    }
  ]
}
